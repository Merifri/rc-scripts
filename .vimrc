" theme
let g:rehash256 = 1
set t_Co=256
let g:molokai_original = 1
filetype indent plugin on
syntax on
colorscheme molokai
" must have
set hidden
set wildmenu
set showcmd
" indent options
set tabstop=2
set shiftwidth=2
set smarttab
set autoindent noexpandtab
set copyindent
set list
set wrap
" find options 
set showmatch
set hlsearch
set incsearch
set ignorecase
set smartcase
" other options
set backspace=indent,eol,start
set nostartofline
set ruler
set laststatus=2
set confirm
set visualbell
set t_vb=
set cursorline
set mouse=a
set cmdheight=2
set number
" key binds
set pastetoggle=<F10>
